<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:geo="http://a9.com/-/opensearch/extensions/geo/1.0/"
  xmlns:time="http://a9.com/-/opensearch/extensions/time/1.0/">
  <ShortName>Geoportal</ShortName>
  <Description>Geoportal</Description>
  <Url type="application/atom+xml" indexOffset="1"
    template="{opensearch.url}?q={searchTerms}&amp;start={startIndex?}&amp;num={count?}&amp;id={geo:uid?}&amp;bbox={geo:box?}&amp;time={time:start?/{time:end?}&amp;f=atom&amp;{args}"/>
  <Url type="application/json" indexOffset="1"
    template="{opensearch.url}?q={searchTerms}&amp;start={startIndex?}&amp;num={count?}&amp;id={geo:uid?}&amp;bbox={geo:box?}&amp;time={time:start?/{time:end?}&amp;f=json&amp;{args}"/>
  <Url type="application/xml" indexOffset="1" rel="results"
    template="{opensearch.url}?q={searchTerms}&amp;start={startIndex?}&amp;num={count?}&amp;id={geo:uid?}&amp;bbox={geo:box?}&amp;time={time:start?/{time:end?}&amp;f=csw&amp;outputschema=http://www.opengis.net/cat/csw/3.0&amp;{args}"/>
  <Query role="example" searchTerms="*:*" />
  <InputEncoding>UTF-8</InputEncoding>
  <OutputEncoding>UTF-8</OutputEncoding>
</OpenSearchDescription>
